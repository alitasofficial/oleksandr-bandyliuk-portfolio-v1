---
interface Props {
  title: string;
  items: Item[];
}

interface Item {
  name: string;
  link: string;
}

const { title, items } = Astro.props;
---

<div class="flex flex-col gap-1">
  <h5 class="header_tertiary">{title}</h5>
  <ul>
    {
      items.map((item) => (
        <li>
          <a href={item.link} class="link">
            {item.name}
          </a>
        </li>
      ))
    }
  </ul>
</div>
